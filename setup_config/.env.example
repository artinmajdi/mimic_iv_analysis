# Environment variables for the MIMIC-IV Analysis project
# Copy this file to .env and fill in your actual values

# Database configuration (for MIMIC-IV connection)
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=password
DB_NAME=mimic_iv

# PhysioNet credentials (for data download if needed)
PHYSIONET_USERNAME=your_username
PHYSIONET_PASSWORD=your_password

# Application settings
DEBUG=true
LOG_LEVEL=INFO
PORT=8501  # Default port for Streamlit
MAX_WORKERS=4

# Paths configuration
MIMIC_IV_DATA_PATH=/path/to/mimic_iv/data
OUTPUT_PATH=./output
MODEL_PATH=./models
